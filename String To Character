public class String_To_Char {
    public static void main(String[] args){
        String s = "hello" ;
        System.out.println("String be " + s) ;
         for(int i = 0; i<s.length() ; i++ ){
             System.out.println("char at index "+i + " be "+ s.charAt(i)) ;
         }
      //  System.out.println(c)  ;
    }
}
